#include <bits/stdc++.h> 
int atoi(string nums) {
    // Write your code here.
    int n = 0 ;
    for(int i = 0 ; i < nums.size();i++)
    {
        if(nums[i] <= '9' && nums[i] >= '0')
        {
            n = n * 10 + (nums[i] - '0') ; 
        }
    }
    if(nums[0] == '-')
    {
        return -n ; 
    }
    return n ; 
}
