#include <bits/stdc++.h> 
void bubbleSort(vector<int>& arr, int n)
{   
    int ind = 0 ; 
    while(ind < n)
    {
        for(int i = 0 ; i < n - ind - 1 ; i++)
        {
            if(arr[i] > arr[i + 1])
            {
                swap(arr[i],arr[i + 1]) ;
            }
        }
        ind++ ; 
    }
    // Write your code here.
}
